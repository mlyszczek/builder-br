BR2_x86_i686=y
BR2_OPTIMIZE_2=y
BR2_TOOLCHAIN_EXTERNAL=y
BR2_TOOLCHAIN_EXTERNAL_PATH="/usr/cross-toolchain/i686-builder-linux-uclibc"
BR2_TOOLCHAIN_EXTERNAL_CUSTOM_PREFIX="i686-builder-linux-uclibc"
BR2_TOOLCHAIN_EXTERNAL_GCC_6=y
BR2_TOOLCHAIN_EXTERNAL_HEADERS_4_9=y
BR2_TOOLCHAIN_EXTERNAL_LOCALE=y
BR2_TARGET_GENERIC_HOSTNAME="bbs-i686-builder-linux-uclibc"
BR2_TARGET_GENERIC_ISSUE="Welcome to i686-builder-linux-ublibc"
BR2_TARGET_GENERIC_PASSWD_SHA256=y
BR2_INIT_SYSV=y
BR2_ROOTFS_DEVICE_CREATION_STATIC=y
BR2_ROOTFS_DEVICE_TABLE="system/device_table.txt board/builder/permission-table.txt"
BR2_TARGET_GENERIC_ROOT_PASSWD="TfxECHodAf9qomH"
BR2_SYSTEM_BIN_SH_BASH=y
# BR2_TARGET_GENERIC_GETTY is not set
BR2_ENABLE_LOCALE_WHITELIST="C en_US pl_PL"
BR2_ROOTFS_USERS_TABLES="board/builder/users"
BR2_ROOTFS_OVERLAY="overlay"
BR2_ROOTFS_POST_BUILD_SCRIPT="board/builder/pre-image.sh"
BR2_LINUX_KERNEL=y
BR2_LINUX_KERNEL_CUSTOM_VERSION=y
BR2_LINUX_KERNEL_CUSTOM_VERSION_VALUE="4.9.66"
BR2_LINUX_KERNEL_USE_CUSTOM_CONFIG=y
BR2_LINUX_KERNEL_CUSTOM_CONFIG_FILE="board/builder/kernel.i686-builder-linux-gnu.config"
BR2_TARGET_ROOTFS_EXT2=y
BR2_TARGET_ROOTFS_EXT2_4=y
BR2_TARGET_ROOTFS_EXT2_SIZE="10g"
# BR2_TARGET_ROOTFS_TAR is not set
